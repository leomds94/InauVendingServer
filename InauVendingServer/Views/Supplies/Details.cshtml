@model InauVendingServer.Models.Supply

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Detalhes</h2>

<div>
    <h4>Suprimento de Produto</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.SupplyQty)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.SupplyQty)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.ProductMachine)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.ProductMachine.ProductMachineQty)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.SupplyTime)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.SupplyTime)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" class="btn btn-default" asp-route-id="@Model.SupplyId">Editar</a>
    <a asp-action="Index" class="btn btn-primary">Voltar para lista</a>
</div>
